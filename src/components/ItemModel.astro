---
interface Props {
  imgUrl: string;
  altData: string;
}
const { imgUrl, altData } = Astro.props;
---

<div class="swiper-slide">
  <img src={imgUrl} alt={altData} class="swiper-slide-img" />
</div>

<style>

  .swiper-slide-img {
    display: block;
    object-fit: cover;
    object-position: center;
    width: 100%;
    height: 100%;
  }
</style>
